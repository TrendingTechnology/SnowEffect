<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SnowEffect">
        <attr name="elementSpeed" format="float"/>
        <attr name="elementShowSpeedMillis" format="integer"/>
        <attr name="elementShowIn"/>
